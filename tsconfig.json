{
  "compilerOptions": {
    "lib": ["dom", "DOM.Iterable", "ESNext"],                                        
    "jsx": "react-jsx",                               
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,                    
    "allowJs": true,                                 
    "allowSyntheticDefaultImports": true,             
    "esModuleInterop": true,                             
    "strict": true,                                       
    "skipLibCheck": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true

  },
    "include": ["src"]

}
